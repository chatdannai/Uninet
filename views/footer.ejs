  <script type="text/javascript">
  	var startTime;
	var endTime;
    $(document).ready(function() {
    	$('#reserve').DataTable( {
           "pagingType": "full_numbers"
	    });
	    $('#historytable2').DataTable( {
	        "pagingType": "full_numbers"
	    });
	    $('#historytable').DataTable( {
            "pagingType": "full_numbers"
        });
        $('#activeservices').DataTable( {
            "pagingType": "full_numbers"
        });
  		 $('#servicesapprove').DataTable( {
            "pagingType": "full_numbers"
        });
  		 $('#servicesrequest').DataTable( {
            "pagingType": "full_numbers"
        });
	    $('#datePicker').daterangepicker({
		    timePicker: true,
		    timePicker24Hour: true,
	        locale: {
	            format: 'YYYY/MM/DD HH:mm:ss'
	        }
	    },
	        function(start, end) {
		        $('#reportrange span').html(start.format('YYYY/MM/DD HH:mm:ss') + ' - ' + end.format('YYYY/MM/DD HH:mm:ss'));
		        startTime = start;
		        endTime = end;
		        document.getElementById("startTime").value=startTime.format('YYYY/MM/DD HH:mm:ss');
		        document.getElementById("endTime").value=endTime.format('YYYY/MM/DD HH:mm:ss');
		    }
		);
      $('#date_selection').daterangepicker({
            singleDatePicker: true,
            showDropdowns: true
          }, 
          function(start, end, label) {
            var years = moment().diff(start, 'years');
            alert("You are " + years + " years old.");
          });
		 $('.modal').on('show.bs.modal', function () {
      if ($(document).height() > $(window).height()) {
        // no-scroll
        $('body').addClass("modal-open-noscroll");
      }
      else { 
        $('body').removeClass("modal-open-noscroll");
      }
    })
    $('.modal').on('hide.bs.modal', function () {
        $('body').removeClass("modal-open-noscroll");
    })
    });
  </script>

