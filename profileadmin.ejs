<!---------------------------------------------------Admin Profile------------------------------------------------>
<!doctype html>
<html>
<head>
    <title>Profile</title>
    <link rel="stylesheet" href="lib/uni.css">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script><!-- load bootstrap css -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> <!-- load fontawesome -->
</head>
<body style="background-color: #006DA6;">
<div class="container-fluid">
<div class="row">
<div class="col-xs-12 nopadding">
<% include ./base %>
</div>
 <div class="col-xs-12 nopadding">
<% include ./menuadmin %>
</div>
<div class="col-xs-2 nopadding"  style="min-width:250px;">
<%include ./admin %>
</div>
<div class="col-xs-10 nopadding">
    <div id="contenttext">
      <div style="margin: 30px;">
       <br><div class="form-group"><label><font size="30px">Profile</font></label>
       <br><div class="form-group">
	    <label class="col-xs-2 control-label"> First Name</label><label class="col-xs-1 control-label"> : </label>
            
		<div class="col-xs-2">
            		<%=user.NameE%>
		</div>
        </div>

        <br><div class="form-group">
            <label class="col-xs-2 control-label"> Last Name</label><label class="col-xs-1 control-label"> : </label>
		<div class="col-xs-4">
            		<%=user.LastNameE%>
		</div>
        </div>

	<br><div class="form-group">
            <label class="col-xs-2 control-label"> Organization</label><label class="col-xs-1 control-label"> : </label>
                <div class="col-xs-4">
        <% if(user.org === 1) { %>
			Chulalongkorn University
       <% } %>
	   <% if(user.org === 2) { %>
			Kasetsart University
       <% } %>
	   <% if(user.org === 3) { %>
			Khon Kean University
       <% } %>
	   <% if(user.org === 4) { %>
			Chiang Mai University
       <% } %>
       <% if(user.org === 5) { %>
			Thaksin University
       <% } %>
	   <% if(user.org === 6) { %>
			King Mongkut's University of Technology Thonburi
       <% } %>
	   <% if(user.org === 7) { %>
			King Mongkut's University of Technology North Bangkok
       <% } %>
	   <% if(user.org === 8) { %>
			Suranaree University of Technology
       <% } %>
       <% if(user.org === 9) { %>
			Thammasat University
       <% } %>
	   <% if(user.org === 10) { %>
			Nakhon Phanom University
       <% } %>
	   <% if(user.org === 11) { %>
			Princess of Naradhiwas University
       <% } %>
	   <% if(user.org === 12) { %>
			Naresuan University
       <% } %>
       <% if(user.org === 13) { %>
			Burapha University
       <% } %>
	   <% if(user.org === 14) { %>
			University of Phayao
       <% } %>
	   <% if(user.org === 15) { %>
			Mahasarakham University
       <% } %>
	   <% if(user.org === 16) { %>
			Mahidol University
       <% } %>
       <% if(user.org === 17) { %>
			Maejo University
       <% } %>
	   <% if(user.org === 18) { %>
			Mae Fah Luang University
       <% } %>
	   <% if(user.org === 19) { %>
			Walailak University
       <% } %>
	   <% if(user.org === 20) { %>
			Srinakharinwirot University
       <% } %>
       <% if(user.org === 21) { %>
			Silpakorn University
       <% } %>
	   <% if(user.org === 22) { %>
			Prince of Songkla University
       <% } %>
	   <% if(user.org === 23) { %>
			Ubon Ratchathani University
       <% } %>
	   <% if(user.org === 24) { %>
			King Mongkut's Institute of Technology Ladkrabang
       <% } %> 
                </div>
        </div>


        <br><div class="form-group">
	    <label class="col-xs-2 control-label"> Phone number</label><label class="col-xs-1 control-label"> : </label>
		<div class="col-xs-3">
            		<%=user.phone%>
		</div>
        </div>

        <br><div class="form-group">
	    <label class="col-xs-2 control-label"> Email</label><label class="col-xs-1 control-label"> : </label>
		<div class="col-xs-3">
            		<%=user.email%>
		</div>
        </div>

        <br><div class="form-group">
	    <label class="col-xs-2 control-label"> Member Type</label><label class="col-xs-1 control-label"> : </label>
		<div class="col-xs-3">Associate Member
		</div>
        </div>

        <br><div class="form-group">
            <label class="col-xs-2 control-label">Role</label><label class="col-xs-1 control-label"> : </label>
		<div class="col-xs-3">Admin
		</div>
        </div>

        <br> 
        </div>
      </div>
    </div>

<div class="col-xs-12 nopadding">
<% include ./footer %>
</div>
</body>
</html>


